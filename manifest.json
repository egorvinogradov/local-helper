{
  "name": "Local Helper",
  "content_scripts": [
    {
      "js": ["gg-reloader.js"],
      "matches": ["http://e.ggtimer.com/*"]
    },
    {
      "js": ["google-redirect.js"],
      "matches": ["https://www.google.com/*"]
    }
  ],
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "version": "1.0.0"
}
