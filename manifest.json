{
  "name": "0 Local Helper",
  "content_scripts": [
    {
      "js": ["gg-reloader.js"],
      "matches": ["http://e.ggtimer.com/*"]
    },
    {
      "js": ["rbc-clipboard.js"],
      "matches": ["https://www.rbc.ru/*"]
    },
    {
      "js": ["google-redirect.js"],
      "matches": ["https://www.google.com/*"]
    },
    {
      "js": ["icons8-copy-svg.js"],
      "matches": ["https://icons8.com/*"]
    },
    {
      "js": ["parse-linkedin-profile.js"],
      "matches": ["https://www.linkedin.com/*"]
    },
    {
      "js": ["libs/jquery.js", "rescuetime-helper.js"],
      "matches": ["https://www.rescuetime.com/*"]
    }
  ],
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "version": "5.0.0",
  "browser_action": {
    "default_title": "Local Helper",
    "default_popup": "popup/popup.html"
  }
}
